<main>
<h1 class="action-header">Login</h1>

<div class="container">
 <div class="row">
   <div class="form">
     <%= form_for(:session,  :url => "login",  :html => {class: "form-horizontal", role: "form",  method: "post"}) do |f| %>
         <%= f.email_field :email, class: "form-control", placeholder: "Email" %>
         <%= f.password_field :password, class: "form-control", placeholder: "Password" %>
         <%= f.submit "Login", class: "btn btn-success" %>
     <% end %>
</div>
</div>
</div>
</main>
